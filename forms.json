{
	"model": "forms",
	"data": [
		{
			"name": "vguide_request",
			"label": "Visitor Guide Request",
			"form_type_id": "contact",
			"is_posted_on_website": true,
			"is_active": true,
			"publication_start_date_at": "2024-06-20T00:00:00.000Z",
			"publication_end_date_at": "2025-06-20T00:00:00.000Z",
			"response_url": "https://simpleviewinc.com",
			"response_text": "Check out the website for more information",
			"is_deleted": false,
			"layout": {
				"fields": [
					{
						"name": "first_name",
						"outkey": "contacts.first_name",
						"type": "text",
						"label": "First Name",
						"required": true
					},
					{
						"name": "last_name",
						"outkey": "contacts.last_name",
						"type": "text",
						"label": "Last Name",
						"required": true
					},
					{
						"name": "email_address",
						"outkey": "contacts.primary_email_address",
						"size": "md",
						"type": "text",
						"label": "Email Address",
						"required": true,
						"validators": [
							"validateEmail"
						]
					},
					{
						"name": "primary_phone",
						"type": "phone",
						"label": "Phone",
						"outkey": "contacts.primary_phone"
					},
					{
						"name": "primary_phone_extension",
						"size": "xs",
						"type": "number",
						"label": "Extension",
						"outkey": "contacts.primary_phone_extension",
						"args": {
							"validatorSettings": {
								"max": 99999,
								"min": 1,
								"wholeNumbers": true
							}
						}
					},
					{
						"name": "mobile_phone",
						"outkey": "contacts.mobile_phone",
						"type": "phone",
						"label": "Mobile"
					},
					{
						"name": "preferred_contact_type",
						"outkey": "contacts.preferred_contact_type",
						"type": "dropdown",
						"label": "Preferred Contact Method",
						"args": {
							"getOptions": {
								"moduleDefName": "dropdowns.preferred_contact_types"
							}
						}
					},
					{
						"name": "type",
						"outkey": "inquiries.type",
						"show": false,
						"type": "dropdown",
						"label": "Type",
						"defaultValue": 5,
						"args": {
							"getOptions": {
								"moduleDefName": "dropdowns.inquiry_types"
							}
						}
					},
					{
						"name": "interests",
						"outkey": "inquiries.interests",
						"type": "checkbox",
						"label": "Interests",
						"args": {
							"getOptions": {
								"moduleDefName": "dropdowns.interests"
							}
						}
					},
					{
						"name": "arrival_date_at",
						"outkey": "inquiries.arrival_date_at",
						"type": "date",
						"label": "Arrival Date",
						"helper_text": "If you are coming to town, when do you arrive",
						"args": {
							"showTime": false
						}
					},
					{
						"name": "custom_dropdown",
						"outkey": "contacts_ext.dropdown",
						"type": "dropdown",
						"label": "Dropdown Field",
						"args": {
							"getOptions": {
								"model": "sf_custom2",
								"schema": "ext",
								"primaryKey": "database_id",
								"primaryKeyType": "identity",
								"labelColumn": "ui_label",
								"sortColumn": "sort_order"
							}
						}
					}
				],
				"sections": [
					{
						"title": "Contact Information",
						"fields": [
							[
								[
									"first_name"
								],
								[
									"last_name"
								]
							],
							[
								[
									"email_address"
								]
							],
							[
								[
									"primary_phone"
								],
								[
									"primary_phone_extension"
								],
								[]
							],
							[
								[
									"mobile_phone"
								],
								[
									"preferred_contact_type"
								]
							]
						]
					},
					{
						"title": "Visit Information",
						"fields": [
							[
								[
									"interests"
								]
							],
							[
								[
									"arrival_date_at"
								]
							],
							[
								[
									"custom_dropdown"
								]
							]
						]
					}
				]
			}
		},
		{
			"name": "newsletter_sign_up",
			"label": "Newsletter Sign Up",
			"form_type_id": "contact",
			"is_posted_on_website": true,
			"is_active": true,
			"publication_start_date_at": "2024-06-20T00:00:00.000Z",
			"publication_end_date_at": "2025-06-20T00:00:00.000Z",
			"response_url": "https://google.com",
			"response_text": "all the information in the world",
			"is_deleted": false
		},
		{
			"name": "partner_sign_up",
			"label": "Partner Sign Up",
			"form_type_id": "partner_signup",
			"is_posted_on_website": true,
			"is_active": true,
			"response_text": "You will receive an email shortly to login...",
			"is_deleted": false,
			"layout": {
				"fields": [
					{
						"name": "first_name",
						"outkey": "account_users.first_name",
						"label": "First Name",
						"type": "text",
						"required": true
					},
					{
						"name": "last_name",
						"outkey": "account_users.last_name",
						"label": "Last Name",
						"type": "text",
						"required": true
					},
					{
						"name": "email_address",
						"outkey": "account_users.email_address",
						"label": "Email Address",
						"type": "text",
						"required": true,
						"size": "md",
						"validators": ["validateEmail"]
					},
					{
						"name": "title",
						"outkey": "account_users.title",
						"label": "Title",
						"type": "text",
						"size": "md"
					},
					{
						"name": "company",
						"outkey": "accounts.company",
						"label": "Company",
						"type": "text",
						"required": true,
						"size": "md"
					},
					{
						"name": "address_line_1",
						"outkey": "accounts.address_line_1",
						"label": "Address",
						"type": "text",
						"required": true,
						"size": "lg"
					},
					{
						"name": "address_line_2",
						"outkey": "accounts.address_line_2",
						"type": "text",
						"size": "lg"
					},
					{
						"name": "address_line_3",
						"outkey": "accounts.address_line_3",
						"type": "text",
						"size": "lg"
					},
					{
						"name": "city",
						"outkey": "accounts.city",
						"label": "City",
						"type": "text",
						"required": true
					},
					{
						"name": "postal_code",
						"outkey": "accounts.postal_code",
						"label": "Postal Code",
						"type": "text",
						"required": true
					},
					{
						"name": "state",
						"outkey": "accounts.state",
						"label": "State",
						"type": "dropdown",
						"required": true,
						"args": {
							"getOptions": {
								"moduleDefName": "states",
								"filter": {
									"country_id": {
										"eq": "US"
									}
								}
							}
						}
					},
					{
						"name": "country",
						"outkey": "accounts.country",
						"label": "Country",
						"type": "dropdown",
						"required": true,
						"size": "md",
						"args": {
							"getOptions": {
								"moduleDefName": "countries"
							}
						}
					}
				],
				"sections": [
					{
						"fields": [
							[
								["first_name"], ["last_name"]
							],
							[
								["email_address", "company", "title"]
							],
							[["address_line_1"]],
							[["address_line_2"]],
							[["address_line_3"]],
							[["country"]],
							[["city"], ["state"], ["postal_code"]]
						]
					}
				]
			}
		}
	]
}