		{
			"name": "Partner Sign Up",
			"form_type_id": "partner_signup",
			"is_posted_on_website":true,
			"is_active":true,
			"response_text":"You will receive an email shortly to login...",
			"is_deleted": false,
			"layout": {
				"fields": [
					{
						"name": "first_name",
						"label": "First Name",
						"type": "text",
						"required": true
					},
					{
						"name": "last_name",
						"label": "Last Name",
						"type": "text",
						"required": true
					},
					{
						"name": "email_address",
						"label": "Email Address",
						"type": "text",
						"required": true,
						"size": "md",
						"validators": ["validateEmail"]
					},
					{
						"name": "title",
						"label": "Title",
						"type": "text",
						"size": "md"
					},
					{
						"name": "company",
						"label": "Company",
						"type": "text",
						"required": true,
						"size": "md"
					},
					{
						"name": "address_line_1",
						"label": "Address",
						"type": "text",
						"required": true,
						"size": "lg"
					},
					{
						"name": "address_line_2",
						"type": "text",
						"size": "lg"
					},
					{
						"name": "address_line_3",
						"type": "text",
						"size": "lg"
					},
					{
						"name": "city",
						"label": "City",
						"type": "text",
						"required": true
					},
					{
						"name": "postal_code",
						"label": "Postal Code",
						"type": "text",
						"required": true
					},
					{
						"name": "state_id",
						"label": "State",
						"type": "dropdown",
						"args": {
							"getOptions": {
								"model": "states",
								"primaryKey": "state_id",
								"labelColumn": "name",
								"filter": {
									"country_id": {
										"eq": "US"
									}
								}
							}
						}
					},
					{
						"name": "country_id",
						"label": "Country",
						"type": "dropdown",
						"required": true,
						"size": "md",
						"args": {
							"getOptions": {
								"model": "countries",
								"primaryKey": "country_id",
								"labelColumn": "name"
							}
						}
					}
				],
				"sections": [
					{
						"fields": [
							[
								["first_name"], ["last_name"]
							],
							[
								["email_address", "company", "title"]
							],
							[["address_line_1"]],
							[["address_line_2"]],
							[["address_line_3"]],
							[["country_id"]],
							[["city"], ["state_id"], ["postal_code"]]
						]
					}
				]
			}
		}
	]
}

